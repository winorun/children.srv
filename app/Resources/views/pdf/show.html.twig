{% extends 'base.html.twig' %}

{% block title %}{{pdf.name}}{% endblock %}

{% block body %}
<div class="col-md-8">
 <div class="page-header">
 <h1>{{pdf.name}}</h1></div>
 <iframe src="/pdfjs/web/viewer.html?file={{ vich_uploader_asset(pdf, 'pdf') }}" 
 		 width="100%" 
 		 height="100%"
 		 style="height:-webkit-fill-available;" 
 		 >
 	<a href="{{ vich_uploader_asset(pdf, 'pdf') }}">{{pdf.name}}</a>
 </iframe>
</div>
{% endblock %}
